<!-- EJS - Template - Verbindung zu Server Data fehlt! -->
<html ng-app="ThingsApp">
<head>
  <meta charset="utf8" />
  <title>ThingsApp</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
</head>
<body>
  <script type="text/javascript" src="node_modules/ejs/ejs.js"></script>
  <div class="container">

    <form>
      <input type="text" ng-model="search">
      <p ng-show="search">Du suchst gerade nach: {{search}}</p>
    </form>

   <table class="table" ng-controller="ObsCtrl">
      <tr ng-repeat="obj in obs | filter:search">
        <td>{{obj.id}}</td>
        <td>{{obj.name}}</td>
        <td>{{obj.dob}}</td>
      </tr>
    </table>

  </div>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.0/angular.js"></script>
  <script>
  angular.module('ThingsApp', [])
  .controller('ObsCtrl', function($scope){
    $scope.obs = obs;
  });
  </script>
</body>
</html>